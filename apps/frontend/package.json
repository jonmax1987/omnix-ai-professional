{
  "name": "omnix-ai-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint .",
    "lint:security": "eslint . --ext .js,.jsx,.ts,.tsx --config eslint-security.config.js",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "vitest run src/test/a11y",
    "test:integration": "vitest run src/test/integration",
    "test:performance": "vitest run src/test/performance",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "optimize:assets": "node scripts/optimize-assets.js",
    "analyze": "npm run build:analyze",
    "size": "size-limit",
    "size:why": "npx bundle-analyzer",
    "build:staging": "cross-env NODE_ENV=production vite build --mode staging",
    "build:production": "cross-env NODE_ENV=production vite build --mode production",
    "build:test": "cross-env NODE_ENV=test vite build --mode test",
    "deploy:staging": "./deploy-multi-env.sh staging",
    "deploy:production": "./deploy-multi-env.sh production",
    "deploy:check": "./check-deployment-status.sh",
    "security:audit": "npm audit --audit-level=moderate",
    "security:scan": "npm run lint:security && npm run security:audit",
    "validate:build": "node scripts/validate-build.js",
    "clean": "rm -rf dist/ node_modules/.vite/ .eslintcache",
    "postinstall": "echo 'OMNIX AI dependencies installed successfully'"
  },
  "dependencies": {
    "@sentry/react": "^10.5.0",
    "@sentry/vite-plugin": "^4.1.1",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@types/chart.js": "^2.9.41",
    "@types/d3": "^7.4.3",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "immer": "^10.1.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lru-cache": "^5.1.1",
    "lucide-react": "^0.540.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-app": "^11.2.0",
    "@squoosh/lib": "^0.3.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.1",
    "playwright": "^1.54.2",
    "puppeteer": "^24.16.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.34.3",
    "size-limit": "^11.2.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-bundle-analyzer": "^1.2.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}
